<VAST version="4.2" xmlns:xs="http://www.w3.org/2001/XMLSchema"
      xmlns="http://www.iab.com/VAST">
    <Ad id="1">
        <InLine>
            <AdSystem>AdSystem Name</AdSystem>
            <AdTitle>Ad Title</AdTitle>
            <Impression>
                <![CDATA[https://localhost:3000/tracker/impression]]></Impression>
            <Creatives>
                <Creative>
                    <Linear>
                        <Duration>00:00:15</Duration>
                        <MediaFiles>
                            <MediaFile type="video/mp4" width="640" height="480" scalable="true"
                                       maintainAspectRatio="true">
                                <![CDATA[https://storage.googleapis.com/dmplayer/ads/vpaid_mediafile1.mp4]]></MediaFile>
                        </MediaFiles>
                        <VideoClicks>
                            <ClickThrough><![CDATA[https://www.dailymotion.com]]></ClickThrough>
                            <ClickTracking>
                                <![CDATA[https://localhost:3000/tracker/clicktracking?click_position=[CLICKPOS]]]></ClickTracking>
                        </VideoClicks>
                        <TrackingEvents event="creativeView">
                            <Tracking event="loaded">
                                <![CDATA[https://localhost:3000/tracker/loaded?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="start">
                                <![CDATA[https://localhost:3000/tracker/start?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="play">
                                <![CDATA[https://localhost:3000/tracker/play?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="pause">
                                <![CDATA[https://localhost:3000/tracker/pause?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="progress" offset="00:00:04">
                                <![CDATA[https://localhost:3000/tracker/progress?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="firstQuartile">
                                <![CDATA[https://localhost:3000/tracker/firstQuartile?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="midpoint">
                                <![CDATA[https://localhost:3000/tracker/midpoint?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="thirdQuartile">
                                <![CDATA[https://localhost:3000/tracker/thirdQuartile?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="complete">
                                <![CDATA[https://localhost:3000/tracker/complete?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="playerExpand">
                                <![CDATA[https://localhost:3000/tracker/playerExpand?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                            <Tracking event="playerCollapse">
                                <![CDATA[https://localhost:3000/tracker/playerCollapse?ad_progress=[ADPLAYHEAD]&player_size=[PLAYERSIZE]&ad_position=[BREAKPOSITION]]]></Tracking>
                        </TrackingEvents>
                    </Linear>
                </Creative>
            </Creatives>
            <Extensions>
                <Extension type="skipoffset"><![CDATA[00:00:05]]></Extension>
            </Extensions>
        </InLine>
    </Ad>
</VAST>
